# Copy this file to .env and fill in the values

# OpenAI / Provider
OPENAI_API_KEY=
# Optional: override model
OPENAI_MODEL=gpt-4o-mini

# dbt configuration (adjust adapter and target as needed)
DBT_TARGET=dev
DBT_PROFILES_DIR=/app/dbt_project

# Example: Snowflake (if using dbt-snowflake)
SNOWFLAKE_ACCOUNT=
SNOWFLAKE_USER=
SNOWFLAKE_PASSWORD=
SNOWFLAKE_ROLE=
SNOWFLAKE_WAREHOUSE=
SNOWFLAKE_DATABASE=
SNOWFLAKE_SCHEMA=

# Example: Postgres (if using dbt-postgres)
PG_HOST=
PG_PORT=5432
PG_USER=
PG_PASSWORD=
PG_DATABASE=
PG_SCHEMA=public
